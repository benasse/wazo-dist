#!/bin/sh

set -e

case "$1" in
	install)
		if [ -f /etc/apt/sources.list.d/xivo-dist.list ] ; then
			rename 's/xivo-dist/wazo-dist' /etc/apt/sources.list.d/xivo-dist.list
		fi
	;;
	upgrade)
	;;

	abort-upgrade)
	;;

	*)
		echo "preinst called with unknown argument \`$1'" >&2
		exit 1
	;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
