#! /bin/sh
# postrm script for xivo-dist
#
# see: dh_installdeb(1)
# remove,

set -e

case "$1" in
    purge|abort-install)
        rm -f /etc/apt/sources.list.d/xivo-dist.list
    ;;
    remove|disappear|upgrade|failed-upgrade|abort-upgrade)

    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
